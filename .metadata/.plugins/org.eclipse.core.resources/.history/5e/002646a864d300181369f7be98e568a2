package com.example.demo.model;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.OneToMany;

import com.fasterxml.jackson.annotation.JsonIgnore;

public class Exam {
	private Integer id;
	private Integer year;
	private Integer sem;
	private String date;
	private String badge;
	@OneToMany(mappedBy="exam")
	@JsonIgnore
	private List<MarkModel> mark=new ArrayList<>();
	public Exam() {
		
	}
	public Exam(Integer id, Integer year, Integer sem, String date, String badge, List<MarkModel> mark) {
		super();
		this.id = id;
		this.year = year;
		this.sem = sem;
		this.date = date;
		this.badge = badge;
		this.mark = mark;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getYear() {
		return year;
	}
	public void setYear(Integer year) {
		this.year = year;
	}
	public Integer getSem() {
		return sem;
	}
	public void setSem(Integer sem) {
		this.sem = sem;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public String getBadge() {
		return badge;
	}
	public void setBadge(String badge) {
		this.badge = badge;
	}
	public List<MarkModel> getMark() {
		return mark;
	}
	public void setMark(List<MarkModel> mark) {
		this.mark = mark;
	}
	
	

}
