package com.example.demo.model;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.OneToMany;

@Entity
public class SubjectModel {
	@Id
	private String sbject_code_num;
	private String subject_name;
	@OneToMany(mappedBy="subject")
	private List<DigreeProgramSubjectModel> digreeProgramSubject=new ArrayList<>();
	
	public SubjectModel() {
		
	}
	public SubjectModel(String sbject_code_num, String subject_name,
			List<DigreeProgramSubjectModel> digreeProgramSubject) {
		super();
		this.sbject_code_num = sbject_code_num;
		this.subject_name = subject_name;
		this.digreeProgramSubject = digreeProgramSubject;
	}





	public String getSbject_code_num() {
		return sbject_code_num;
	}
	public void setSbject_code_num(String sbject_code_num) {
		this.sbject_code_num = sbject_code_num;
	}
	public String getSubject_name() {
		return subject_name;
	}
	public void setSubject_name(String subject_name) {
		this.subject_name = subject_name;
	}
	public List<DigreeProgramSubjectModel> getDigreeProgramSubject() {
		return digreeProgramSubject;
	}
	public void setDigreeProgramSubject(List<DigreeProgramSubjectModel> digreeProgramSubject) {
		this.digreeProgramSubject = digreeProgramSubject;
	}
	
	

}
